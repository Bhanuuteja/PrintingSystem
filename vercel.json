{
  "version": 2,
  "headers": [
    {
      "source": "/index.tsx",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript; charset=utf-8" },
        { "key": "Cache-Control", "value": "no-store, max-age=0, must-revalidate" },
        { "key": "X-Vercel-Debug-Rule", "value": "Applied-Rule-Index-TSX" }
      ]
    },
    {
      "source": "/(.*\\.tsx)",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript; charset=utf-8" },
        { "key": "Cache-Control", "value": "no-store, max-age=0, must-revalidate" },
        { "key": "X-Vercel-Debug-Rule", "value": "Applied-Rule-Any-TSX" }
      ]
    },
    {
      "source": "/(.*\\.ts)",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript; charset=utf-8" },
        { "key": "Cache-Control", "value": "no-store, max-age=0, must-revalidate" },
        { "key": "X-Vercel-Debug-Rule", "value": "Applied-Rule-Any-TS" }
      ]
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
