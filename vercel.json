{
  "version": 2,
  "headers": [
    {
      "source": "/index\\.tsx",
      "headers": [
        { "key": "Content-Type", "value": "text/javascript; charset=utf-8" },
        { "key": "X-Vercel-Debug-Path", "value": "Matched-Regex-/index.tsx" }
      ]
    },
    {
      "source": "/components/(.+\\.tsx)",
      "headers": [
        { "key": "Content-Type", "value": "text/javascript; charset=utf-8" },
        { "key": "X-Vercel-Debug-Path", "value": "Matched-Regex-/components/xxx.tsx" }
      ]
    },
    {
      "source": "/services/(.+\\.ts)",
      "headers": [
        { "key": "Content-Type", "value": "text/javascript; charset=utf-8" },
        { "key": "X-Vercel-Debug-Path", "value": "Matched-Regex-/services/xxx.ts" }
      ]
    },
    {
      "source": "/([^/]+\\.ts)", 
      "headers": [
        { "key": "Content-Type", "value": "text/javascript; charset=utf-8" },
        { "key": "X-Vercel-Debug-Path", "value": "Matched-Regex-TopLevel.ts" }
      ]
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
